{
    "experiment": "IceCube",
    "group":  "muon_final", 

    "model_name":  "SageHopE",
    "wandblog": true,

    "run_params":{
    "epochs":     30,
    "batch_size": 512,
    "early_stop": true,
    "zeniazi_metric":true,
    "patience":   5,
    "lr_schedule": "classic",
    "learning_rate":   1e-3,
    "warm_up": 3,
    "lr_decay": 0.9,
    "val_epoch":   10,
    "loss_func": "abs_vonMises2D_angle",
    "metrics": "energy_angle_zeniazi",
    "performance_plot": "performance_vM2D"
},


    "hyper_params": {
        "n_out":           3,
        "n_sigs":          2,
        "hidden_states":   30,
        "conv_layers": 2,
        "decode_layers":   3,
        "decode_activation": 1,
        "dropout":         0.03,
        "regularization": "l2"
    },
    "oldn_data": 1.4999e6,
   "data_params":{ 
    "n_data": 1.4999e6,
    "graph_construction":       "classic_lc",
    "muon":             true,
    "n_neighbors":       14,
    "data_split":      [0.8, 0.1, 0.1],
    "restart":   false,
    "transform_path": "../db_files/muongun/transformers.pkl",
    "db_path": "../db_files/muongun/rasmus_classification_muon_3neutrino_3mio.db",
    "features":   ["dom_x", "dom_y", "dom_z", "time", "charge_log10", "SRTInIcePulses", "lc"],
    "targets":    ["energy_log10", "zenith","azimuth"],
    "skip": 0,
    "SRT": 0,
    "database": "MuonGun"
}
}
   